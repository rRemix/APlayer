<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="CircleImageView">
    <attr format="dimension" name="border_width"/>
    <attr format="color" name="border_color"/>
  </declare-styleable>

  <declare-styleable name="CircleSeekBar">
    <attr format="reference" name="progress_thumb"/>
    <attr format="dimension|reference" name="progress_width"/>
    <attr format="color|reference" name="circle_color"/>
    <attr format="integer|reference" name="progress_max"/>
  </declare-styleable>

  <!--<declare-styleable name="ColumnView">-->
    <!--<attr format="color" name="columncolor"/>-->
    <!--<attr format="dimension|integer|reference" name="columnwidth"/>-->
    <!--<attr format="integer" name="columnnum"/>-->
  <!--</declare-styleable>-->

  <declare-styleable name="FilterSizeSeekBar">
    <attr format="dimension|reference" name="trackheight"/>
    <attr format="color|reference" name="trackcolor"/>
    <attr format="color|reference" name="progresscolor"/>
    <attr format="integer" name="dotnum"/>
    <attr format="dimension|reference" name="dotwidth"/>
    <attr format="reference" name="thumb"/>
    <attr format="reference" name="thumbpress"/>
    <attr format="color|reference" name="dotcolor"/>
    <attr format="color|reference" name="tiptextcolor"/>
  </declare-styleable>

  <declare-styleable name="EQSeekBar">
    <attr format="dimension|reference" name="eqtrackwidth"/>
    <attr format="color|reference" name="eqenabletrackcolor"/>
    <attr format="color|reference" name="eqenableprogresscolor"/>
    <attr format="color|reference" name="eqtrackcolor"/>
    <attr format="color|reference" name="eqprogresscolor"/>
    <attr format="integer" name="eqdotnum"/>
    <attr format="reference" name="eqthumb"/>
    <attr format="color|reference" name="eqenabletextcolor"/>
    <attr format="color|reference" name="eqtextcolor"/>
  </declare-styleable>

  <declare-styleable name="FastScrollRecyclerView">
    <attr format="boolean" name="hideScrollbar"/>
    <attr format="boolean" name="showTrack"/>
    <attr format="color|reference" name="trackColor"/>
    <attr format="color|reference" name="handleColor"/>
    <attr format="color|reference" name="bubbleColor"/>
    <attr format="color|reference" name="bubbleTextColor"/>
  </declare-styleable>

  <attr format="color|reference" name="text_color_primary"/>
  <attr format="color|reference" name="text_color_secondary"/>
  <attr format="color|reference" name="background_color_main"/>
  <attr format="color|reference" name="background_color_dialog"/>
  <attr format="color|reference" name="background_library_color"/>
  <!--点击效果-->
  <attr format="reference" name="background_grid"/>
  <attr format="reference" name="background_list"/>
  <attr format="reference" name="background_rect_ripple"/>
  <attr format="reference" name="background_oval_ripple"/>

  <!--默认专辑和艺术家封面-->
  <attr format="reference" name="default_album"/>
  <attr format="reference" name="default_artist"/>
  <!--波纹颜色-->
  <attr format="reference|color" name="ripple_color"/>
  <!--分割线-->
  <attr format="color|reference" name="divider_color"/>
